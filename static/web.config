<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <clear />
      
      <add name="StaticFile" 
      path="*" 
      verb="*" 
      modules="StaticFileModule" 
      resourceType="File" 
      requireAccess="Read" />
    </handlers>
	<staticContent>
		<remove fileExtension=".woff2" />
		<mimeMap fileExtension=".woff2" mimeType="font/woff2" />
	  </staticContent>
  </system.webServer>
</configuration>